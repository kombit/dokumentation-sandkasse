<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:kontekst="http://kombit.dk/xml/schemas/kontekst/2017/01/01/" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://kombit.dk/xml/schemas/kontekst/2017/01/01/" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xs:element name="RequestId" type="kontekst:RequestIdType">
		<xs:annotation>
			<xs:documentation>En UUID der unik identificerer de enkelte kald og eventuelle genkald.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:simpleType name="RequestIdType">
	  <xs:restriction base="xs:string">
            <xs:pattern value="[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}"/>
          </xs:restriction>
	</xs:simpleType>
</xs:schema>
