         <xs:include schemaLocation="../operational/KlassifikationPubliceretFraDato.xsd"/>
         
         <xs:include schemaLocation="../operational/KlassifikationPubliceretTilDato.xsd"/>
         
         <xs:include schemaLocation="../view/FacetStrukturType.xsd"/>
         
